ifndef MLD_MODE
       $(warning MLD_MODE not set -- defaulting to MLD_MODE=3)
endif

MLD_MODE ?= 3

ifeq ($(MLD_MODE),2)
     MLD_NAMESPACE=MLD_44_ref_
else ifeq ($(MLD_MODE),3)
     MLD_NAMESPACE=MLD_65_ref_
else ifeq ($(MLD_MODE),5)
     MLD_NAMESPACE=MLD_87_ref_
else
     $(error Invalid value of MLD_MODE)
endif
